{
  "name": "@nervous/demos",
  "type": "module",
  "version": "0.0.1",
  "description": "nervous demos",
  "scripts": {
    "dev": "concurrently \"pnpm run esbuild-watch\" \"npx tailwindcss -o src/dist/output.css --watch\" \"live-server src/ --port=3333 --no-browser\"",
    "esbuild-watch": "node esbuild.cjs watch",
    "build": "node esbuild.cjs && npx tailwindcss -o src/dist/output.css"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@types/p5": "^1.4.3",
    "concurrently": "^7.4.0",
    "live-server": "^1.2.2",
    "tailwindcss": "^3.2.1"
  },
  "dependencies": {
    "esbuild": "^0.15.10",
    "nervous": "workspace:*",
    "p5": "^1.4.2",
    "pako": "^2.0.4"
  }
}