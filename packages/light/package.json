{
	"name": "light",
	"version": "0.0.0",
	"exports": {
		"./package.json": "./package.json",
		".": "./src/index.ts"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/typio/light",
		"directory": "packages/light"
	},
	"files": [
		"src",
		"types"
	],
	"scripts": {
		"build": "tsc --declaration --emitDeclarationOnly --declarationDir types ./src/index.ts",
		"test": "uvu -r tsm test",
		"bench": "tsx bench/tensor.ts",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@types/benchmark": "^2.1.2",
		"benchmark": "^2.1.4",
		"prettier": "^2.6.2",
		"tsx": "3.9.0",
		"typescript": "^4.8.4",
		"uvu": "^0.5.6"
	},
	"types": "types/index.d.ts"
}